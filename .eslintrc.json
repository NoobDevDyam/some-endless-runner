{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "never"],
        "no-console": "off",
        "no-unused-vars": "warn",
        "linebreak-style": "off",
        "comma-dangle": "off"

    }
}
